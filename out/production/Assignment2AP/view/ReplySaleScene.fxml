<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="141.0" prefWidth="600.0" style="-fx-background-color: E0E0E0;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReplyController">
   <children>
      <Label layoutX="45.0" layoutY="20.0" prefHeight="19.0" prefWidth="312.0" text="Please Enter Offer Value and Push Submit Button: ">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="offer" layoutX="395.0" layoutY="17.0" />
      <Button layoutX="54.0" layoutY="58.0" mnemonicParsing="false" onAction="#replySaleButtonPushed" text="Submit" />
      <Label fx:id="saleResult" layoutX="54.0" layoutY="95.0" prefHeight="25.0" prefWidth="528.0">
         <font>
            <Font size="13.0" />
         </font></Label>
   </children>
</AnchorPane>
